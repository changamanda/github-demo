<div class = "container">
  <h1><%= session[:username] %>'s Repos</h1>
  <% @response_array.each do |response| %>
    <%= link_to response["name"], response["html_url"] %><br>
  <% end %><br>

  <%= form_tag("/repositories/create", method: "post") do %>
    <%= label_tag(:name, "Create a New Repo:") %>
    <%= text_field_tag(:name) %>
    <%= submit_tag("Create") %>
  <% end %>
</div>